generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Equipamento {
  id          Int       @id @default(autoincrement())
  nome        String
  serial      String?
  mac         String?
  status      String    @default("estoque")
  destino     String?
  dataEntrada DateTime  @default(now())
  dataSaida   DateTime?
}
